{
  "name": "edm00.se",
  "version": "1.1.0",
  "description": "a url shortener-like project",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "jest --coverage",
    "format": "prettier-eslint \"**/*.js\" --ignore \"static/**/*.js\" --write",
    "doc": "apidoc -o static/docs -i routes"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/edm00se/edm00.se.git"
  },
  "keywords": [
    "node",
    "express",
    "url"
  ],
  "author": "Eric McCormick (https://ericmccormick.io)",
  "license": "MIT",
  "homepage": "https://bitbucket.org/edm00se/edm00.se#readme",
  "dependencies": {
    "clean-urls": "1.1.5",
    "compression": "1.6.2",
    "edm00se-urls": "1.0.1",
    "ejs": "2.5.6",
    "express": "4.15.3",
    "express-toobusy": "0.0.3",
    "nodalytics": "1.1.0"
  },
  "devDependencies": {
    "apidoc": "0.17.6",
    "chai": "4.0.2",
    "eslint": "4.0.0",
    "eslint-config-xo-space": "0.16.0",
    "husky": "0.13.4",
    "jest": "20.0.4",
    "prettier-eslint-cli": "4.1.1",
    "supertest": "3.0.0"
  },
  "eslintConfig": {
    "extends": "xo-space",
    "env": {
      "node": true,
      "jest": true
    }
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 90,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
